@model Consignment_Control.Models.Settings.ConsigneeDetailsModel

@{
    ViewBag.Title = "StockTake";

}

@section Breadcrumbs
{
    <div class="navbar-wrapper">
        <a class="navbar-brand" href="javascript:;">Settings - Edit Consignee</a>
    </div>

}


@section Content
{
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        @Html.HiddenFor(m => m.ID)
        <div class="row">
            <div class="card">

                <div class="card-body text-right">
                    <button type="submit" class="btn btn-primary" id="submit-button" name="submit-button" value="submit-button">Update</button>
                    @*<button type="submit" class="btn btn-primary" id="print-button" name="print-button" value="print-button">Print PDF</button>

                        <a href="@Url.RouteUrl("ViewStockTake")" class="btn btn-primary">View</a>*@
                </div>
            </div>
            <div class="card">
                <div class="card-header card-header-primary">
                    <div class="row">
                        <div class="col-md-6 justify-content-center">
                            <h4 class="card-title text-left">Edit Consignee</h4>
                        </div>



                    </div>
                </div>
                <div class="card-body">
                    <form>
                        <div class="row">



                            <div class="col-md-6">
                                <div class="form-group">
                                    @Html.LabelFor(m => m.Ah_code)
                                    @Html.EditorFor(model => model.Ah_code, new { htmlAttributes = new { @class = "cust-FormControl", @readonly = "readonly" } })
                                </div>

                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    @Html.LabelFor(m => m.Ah_name)
                                    @Html.EditorFor(model => model.Ah_name, new { htmlAttributes = new { @class = "cust-FormControl" } })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">

                                    @Html.LabelFor(m => m.Status)
                                    @Html.DropDownListFor(m => m.Status, Model.SelectStatus, new { @class = "cust-FormControl" })
                                </div>

                            </div>

                        </div>

                        @*<div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">

                                        <label class="bmd-label-floating">Remark</label>
                                        <input type="text" class="cust-FormControl">

                                    </div>
                                </div>
                            </div>*@


                    </form>
                </div>
            </div>
        </div>
    }
}


